                                <img src="assets/img/shapes/section-title-star.png" alt="image not found">
                            </div>
                            <p class="section__subtitle-text">
                                @Model.Value("homeProcessTitle")
                            </p>
                        </div>
                        <h2 class="section__title-main xlarge uppercase tpl__item-center title-anim">
                            @Model.Value("homeProcessSubTitle")
                        </h2>
                    </div>
                </div>
            </div>
            <div class="work-process-inner">
                @{


                    if (coursesPage is Courses coursesContentStep)
                    {
                        var testimonials = coursesContentStep.CoursesSteps;

                        if (testimonials != null && testimonials.Any())
                        {
                            @Html.GetBlockListHtml(coursesContentStep, "coursesSteps", "HomeStep")

