                        <div class="portfolio-slide-3 p-relative wf_panel">
                            <div class="container">
                                <div class="row">
                                    <div class="col-xl-7 col-lg-7">
                                        <div class="portfolio-slide-3-content section-spacing">
                                            <div class="portfolio__num-count"></div>
                                            <h2 class="xlarge uppercase">@Model.Value("homeProjectCardTitle")</h2>
                                            <p>@Model.Value("homeProjectCardSubTitle").</p>
                                            <div class="area-btn btn_wrapper">
                                            <a class="c-button btn-moving" href="@ProjectstPage.Url()">
                                                    <span class="c-button__border"></span>
                                                    <span class="c-button__text">Explor More</span>
                                                </a>
                                    @* <span>Or</span>
                                                 <span class="call-number">Call us - <a href="tel:+0169850023601">+01
                                                        6985 002 3601</a></span> *@
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xl-5 col-lg-5">
                                        <div class="portfolio-slide-3-thumb">
                                            <img src="assets/img/gallery/gallery-1.html" alt="image not found">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>


                    </div>
                </section>
                <!-- portfolio area end  -->
